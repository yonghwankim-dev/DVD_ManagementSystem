function openInsertPopUp()
{
	const url_ = "/DVD/index/dvd_admin/insert";
	const name = "InsertDVD";
	const option = "width = 500, height = 800, top = 100, left = 200, location=no";
	
	const parmas = $("#insert_form").serialize();
	$.ajax({
		url : '<c:url value='+url_+'/>',
		data : params,
		success : function(xh){window.close();}
	});
}